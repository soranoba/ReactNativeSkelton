{
  "name": "ReactNativeSkelton",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/runtime": "7.0.0-beta.47",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-transform-runtime": "7.0.0-beta.3",
    "babel-preset-react-native": "~5.0.2",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "~0.28.7",
    "eslint": "~4.19.1",
    "eslint-config-universe": "~1.0.7",
    "file-loader": "~1.1.7",
    "jest": "~23.4.2",
    "jest-react-native": "~18.0.0",
    "koa-connect": "^2.0.1",
    "prettier": "~1.14.0",
    "react-test-renderer": "~16.3.1",
    "style-loader": "~0.19.0",
    "webpack-cli": "~3.1.0",
    "webpack-serve": "~2.0.2"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "web": "webpack-serve --config ./webpack.config.js --port 8080",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
    "lint": "eslint --max-warnings 0 --ext .js src/ test/ scripts/",
    "format": "eslint --fix --max-warnings 0 --ext .js src/ test/ scripts/"
  },
  "jest": {
    "projects": [
      {
        "displayName": "native",
        "preset": "react-native",
        "testMatch": [
          "<rootDir>/test/**/*.(native|ios|android).test.js"
        ],
        "moduleNameMapper": {
          "^/(.*)$": "<rootDir>/src/$1",
          "^react-native-vector-icons/FontAwesome5$": "react-native-vector-icons/FontAwesome"
        },
        "moduleFileExtensions": ["js", "native.js", "ios.js", "android.js"],
        "transform": {
          "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
        },
        "transformIgnorePatterns": []
      },
      {
        "displayName": "web+shared",
        "preset": "react-native",
        "testMatch": [
          "<rootDir>/test/**/*.js"
        ],
        "testPathIgnorePatterns": [
          "\\.(native|ios|android)\\.test\\.js$"
        ],
        "testURL": "http://localhost/",
        "moduleNameMapper": {
          "^/(.*)$": "<rootDir>/src/$1",
          "^react-native((/.*)?)$": "react-native-web$1",
          "^react-router-dom/BrowserRouter$": "react-router/MemoryRouter",
          "^react-router-native((/.*)?)$": "react-router-dom$1"
        },
        "moduleFileExtensions": ["js", "web.js"],
        "transform": {
          "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
        },
        "transformIgnorePatterns": []
      }
    ]
  },
  "dependencies": {
    "native-base": "~2.7.2",
    "react": "~16.4.1",
    "react-dom": "~16.4.1",
    "react-native": "~0.56.0",
    "react-native-vector-icons": "~5.0.0",
    "react-native-web": "~0.4.0",
    "react-router-dom": "~4.3.1",
    "react-router-native": "~4.3.0",
    "webpack": "~4.16.4"
  }
}
